{
  "tasks": {
    "dev": "./mvnw quarkus:dev",
    "test": "deno task up  && ./mvnw test",
    "test:unit": "./mvnw test -Dtest='!*IntegrationTest'",
    "build": "./mvnw clean package",
    "native": "./mvnw clean package -Pnative",
    "up": "docker compose up -d",
    "down": "docker compose down",
    "ps": "docker compose ps",
    "init": "./mvnw clean compile",
    "lint": "./mvnw spotless:check",
    "format": "./mvnw spotless:apply"
  }
}